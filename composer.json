{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.211",
        "blade-ui-kit/blade-ui-kit": "^0.4",
        "fakerphp/faker": "^1.9.1",
        "gehrisandro/tailwind-merge-laravel": "^1.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.8",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^3.4",
        "meilisearch/meilisearch-php": "^1.6",
        "spatie/commonmark-wire-navigate": "^1.0",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-feed": "^4.2",
        "spatie/laravel-flash": "^1.9",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-log-dumper": "^1.4",
        "spatie/laravel-markdown": "^2.5",
        "spatie/laravel-menu": "^4.1",
        "spatie/laravel-site-search": "^1.0.0",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-tail": "^4.4",
        "spatie/sheets": "^1.11",
        "spatie/spatie-content-api": "^3.0.0",
        "spatie/spatie-price-api": "^1.0",
        "spatie/yaml-front-matter": "^2.0",
        "symfony/http-client": "^6.2",
        "symfony/postmark-mailer": "^6.2",
        "symfony/yaml": "^6.2",
        "tempest/highlight": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "friendsofphp/php-cs-fixer": "^3.6",
        "laravel/envoy": "^2.8",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.33",
        "spatie/laravel-ray": "^1.32"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
